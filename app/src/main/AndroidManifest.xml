<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.patryk.cookbook" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity_"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.patryk.cookbook.LoginActivity_"/>
        <activity android:name="com.example.patryk.cookbook.AddActivity_"/>
        <activity android:name="com.example.patryk.cookbook.RegisterActivity_"/>
        <activity android:name="com.example.patryk.cookbook.DetailActivity_"/>
        <activity android:name="com.example.patryk.cookbook.CommentsActivity_"/>
    </application>
    <uses-permission android:name="android.permission.INTERNET" />
</manifest>
